<sqlMap namespace="BL0001">
    
    <select id="findAll" resultClass="sample.BL0001.BL0001Data">
        SELECT
            ID as id,
            ITEM as item,
            PRICE as price,
            CUSTOMER as customer
        FROM
            SALES
